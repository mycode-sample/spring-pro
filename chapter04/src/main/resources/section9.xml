<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans.xsd"
>
  <bean id="teacher"
        class="com.sheepfly.springpro.chapter4.section9.UserFactoryBean">
    <property name="userType" value="teacher"/>
  </bean>
  <bean id="student"
        class="com.sheepfly.springpro.chapter4.section9.UserFactoryBean">
    <property name="userType" value="student"/>
  </bean>
  <bean id="president"
        class="com.sheepfly.springpro.chapter4.section9.UserFactoryBean">
    <property name="userType" value="president"/>
  </bean>
</beans>